{
    "name": "lukasz93p/object-serializer",
    "description": "Package which allows easy serialization and deserialization of php objects using JSON format, setters or public properties are not needed.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz93P",
            "email": "lukasz9.3@interia.pl"
        }
    ],
    "keywords": [
        "serializer",
        "serialization",
        "deserializer",
        "deserialization",
        "object serialization"
    ],
    "autoload": {
        "psr-4": {
            "Lukasz93P\\objectSerializer\\": "src/"
        }
    },
    "require": {
        "php": "^7.3",
        "doctrine/annotations": "^1.8",
        "jms/serializer": "^3.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "minimum-stability": "stable"
}
